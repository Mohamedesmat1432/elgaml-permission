{
    "name": "elgaml/permission",
    "description": "Role and permission management for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Elgaml\\Permission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elgaml\\Permission\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elgaml\\Permission\\Providers\\PermissionServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/auth": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
